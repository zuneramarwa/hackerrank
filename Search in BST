class Solution:
    def searchBST(self, root: Optional[TreeNode], key: int) -> Optional[TreeNode]:
        if root == None:
            return None
        if root.val < key:
            return self.searchBST(root.right,key)
        elif root.val > key:
            return self.searchBST(root.left,key)
        else:
            return root


        
