class Solution:
    def isSameTree(self, p: Optional[TreeNode], q: Optional[TreeNode]) -> bool:
        if p == None and q == None:
            return True
        elif p == None or q == None:
            return False
        else:
            return (q.val == p.val) and self.isSameTree(p.left,q.left)and self.isSameTree(p.right,q.right)
        
        
