class Solution:
    def inorder(self,root,i,k,answer):
        if root == None:
            return
        self.inorder(root.left,i,k,answer)
        i[0] = i[0]+1
        if i[0] == k:
            answer.append(root.val)
            return
        self.inorder(root.right,i,k,answer)
    def kthSmallest(self, root: Optional[TreeNode], k: int) -> int:
        answer = []
        i = [0]
        self.inorder(root,i,k,answer)
        return answer[0]

        
